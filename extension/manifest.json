{
  "manifest_version": 3,
  "name": "ScrapeHero",
  "version": "1.0",
  "description": "One Click Web Scraper",
  "permissions": ["activeTab", "scripting", "storage"],
  "background": { "service_worker": "background.js" },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  }
}
